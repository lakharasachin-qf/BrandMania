ng itemView) {\r\n            super(itemView.getRoot());\r\n            binding = itemView;\r\n\r\n        }\r\n    }\r\n\r\n    static class CardHolderThree extends RecyclerView.ViewHolder {\r\n        ItemDigitalCardOneBinding binding;\r\n\r\n        CardHolderThree(ItemDigitalCardOneBinding itemView) {\r\n            super(itemView.getRoot());\r\n            binding = itemView;\r\n\r\n        }\r\n    }\r\n\r\n}\r\n
