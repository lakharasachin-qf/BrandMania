<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    xmlns:app="http://schemas.android.com/tools"
    xmlns:shimmer="http://schemas.android.com/apk/res-auto"
    app:ignore="NamespaceTypo">
<RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent">
        <androidx.appcompat.widget.Toolbar
            android:visibility="visible"
            android:id="@+id/my_toolbar"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_70sdp"
            android:background="@drawable/toolbar_background_second"
            android:elevation="4dp"
            android:theme="@style/ThemeOverlay.AppCompat.ActionBar"
            app:popupTheme="@style/ThemeOverlay.AppCompat.Light">
                <RelativeLayout
                    android:layout_marginTop="@dimen/_10sdp"
                    android:layout_marginRight="@dimen/_7sdp"
                    android:id="@+id/main_linearlayout_title"
                    android:elevation="0dp"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_70sdp">
                        <TextView
                            android:id="@+id/firsttitle"
                            android:text="@string/yourbusiness_text"
                            android:textColor="@color/colorNavText"
                            android:textSize="@dimen/_6sdp"
                            android:fontFamily="@font/montserrat_light"
                            android:layout_width="wrap_content"
                            android:textStyle="bold"
                            android:layout_height="wrap_content">

                        </TextView>
                        <RelativeLayout
                            android:id="@+id/businessNameDropDown"
                            android:layout_marginTop="@dimen/_10sdp"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">
                        <TextView

                            android:id="@+id/businessName"
                            android:text="QueryFinderSolution"
                            android:textColor="@color/colorNavText"
                            android:textSize="@dimen/_12sdp"

                            android:fontFamily="@font/montserrat_semibold"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">
                        </TextView>



                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_toEndOf="@+id/businessName"
                            android:src="@drawable/ic_outline_expand_more_24"
                            android:layout_height="wrap_content">

                        </ImageView>
                        </RelativeLayout>
                        <ImageView
                            android:id="@+id/showNotification"
                            android:layout_width="wrap_content"
                            android:layout_alignParentEnd="true"
                            android:visibility="visible"
                            android:src="@drawable/ic_baseline_notifications_none_24"
                            android:layout_height="wrap_content">

                        </ImageView>

                </RelativeLayout>
        </androidx.appcompat.widget.Toolbar>
        <ImageView
            android:layout_marginStart="-100dp"
            android:layout_alignParentBottom="true"

            android:src="@drawable/bottom"
            android:layout_width="@dimen/_300sdp"
            android:layout_height="@dimen/_300sdp">

        </ImageView>


        <androidx.swiperefreshlayout.widget.SwipeRefreshLayout
            android:id="@+id/swipeContainer"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/my_toolbar"
            android:scrollbars="none">
        <androidx.core.widget.NestedScrollView

            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:clipToPadding="false"
            android:scrollbars="none"
            app:behavior_overlapTop="50dp"
            app:layout_behavior="@string/appbar_scrolling_view_behavior">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                        <RelativeLayout
                            android:layout_marginTop="@dimen/_10sdp"
                            android:id="@+id/viewpagerRelativ"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">
                                <androidx.cardview.widget.CardView
                                    android:layout_marginRight="@dimen/_7sdp"
                                    android:layout_width="match_parent"
                                    android:layout_marginLeft="@dimen/_7sdp"
                                    android:layout_height="@dimen/_140sdp"
                                    card_view:cardCornerRadius="@dimen/_10sdp">
                                        <androidx.viewpager.widget.ViewPager
                                            android:id="@+id/ViewPagerView"
                                            android:layout_width="match_parent"
                                            android:layout_height="@dimen/_140sdp"
                                            android:visibility="visible" />
                                </androidx.cardview.widget.CardView>

                        </RelativeLayout>
                        <RelativeLayout
                            android:visibility="gone"
                            android:background="@android:color/transparent"
                            android:layout_below="@+id/viewpagerRelativ"
                            android:layout_marginTop="@dimen/_7sdp"
                            android:id="@+id/alertForFrmae"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">

                                        <RelativeLayout
                                            android:background="@android:color/transparent"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content">
                                        <RelativeLayout
                                            android:background="@android:color/transparent"
                                            android:id="@+id/alertRelative"
                                            android:layout_marginLeft="@dimen/_5sdp"
                                            android:layout_marginRight="@dimen/_5sdp"
                                            android:layout_gravity="center"
                                            android:gravity="center"

                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content">

                                                <TextView
                                                    android:gravity="center"
                                                    android:layout_centerHorizontal="true"
                                                    android:layout_marginTop="@dimen/_5sdp"
                                                    android:textColor="@color/colorPrimary"
                                                    android:id="@+id/alertText2"
                                                    android:textSize="@dimen/_10sdp"
                                                    android:singleLine="true"
                                                    android:ellipsize="marquee"
                                                    android:marqueeRepeatLimit="marquee_forever"
                                                    android:focusable="true"
                                                    android:focusableInTouchMode="true"
                                                    android:scrollHorizontally="true"
                                                    android:fontFamily="@font/montserrat_semibold"
                                                    android:text=""
                                                    android:layout_width="match_parent"
                                                    android:layout_height="wrap_content" />
                                                <LinearLayout
                                                    android:visibility="gone"
                                                    android:layout_marginTop="@dimen/_5sdp"
                                                    android:layout_marginBottom="@dimen/_5sdp"
                                                    android:layout_centerHorizontal="true"
                                                    android:orientation="horizontal"
                                                    android:layout_below="@+id/alertText2"
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="wrap_content">

                                                        <ImageView
                                                            android:visibility="gone"
                                                            android:id="@+id/call"
                                                            android:src="@drawable/ic_phone"
                                                            android:layout_width="@dimen/_18sdp"
                                                            android:layout_height="@dimen/_18sdp" />

                                                        <ImageView
                                                            android:id="@+id/whatsapp"
                                                            android:layout_marginStart="@dimen/_5sdp"
                                                            android:src="@drawable/ic_whatsapp"
                                                            android:layout_width="@dimen/_18sdp"
                                                            android:layout_height="@dimen/_18sdp" />
                                                </LinearLayout>
                                        </RelativeLayout>
                                        </RelativeLayout>
                        </RelativeLayout>

                        <TextView
                            android:id="@+id/creatPdf"
                            android:layout_centerHorizontal="true"
                            android:layout_below="@+id/viewpagerRelativ"
                            android:text="Creat PDF"
                            android:textColor="@color/red"
                            android:textSize="@dimen/_12sdp"
                            android:fontFamily="@font/inter_semibold"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">

                        </TextView>
                        <RelativeLayout

                            android:layout_marginTop="@dimen/_15sdp"
                            android:layout_below="@+id/creatPdf"
                            android:id="@+id/recommendationRelativ"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">
                                <androidx.recyclerview.widget.RecyclerView
                                    android:layout_marginTop="@dimen/_10sdp"
                                    android:id="@+id/rocommRecycler"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_marginBottom="@dimen/_1sdp"
                                    android:paddingBottom="@dimen/_30sdp"
                                    android:clipToPadding="false" />
                                <com.facebook.shimmer.ShimmerFrameLayout

                                    android:layout_below="@+id/rocommRecycler"
                                    android:id="@+id/shimmerForPagination"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:orientation="vertical"
                                    shimmer:shimmer_duration="800">
                                        <LinearLayout
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:orientation="vertical">
                                                <include layout="@layout/item_simmer_visitors_layout" />
                                                <include layout="@layout/item_simmer_visitors_layout" />
                                        </LinearLayout>

                                </com.facebook.shimmer.ShimmerFrameLayout>

                        <com.facebook.shimmer.ShimmerFrameLayout

                            android:id="@+id/shimmer_view_container"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:orientation="vertical"
                            shimmer:shimmer_duration="800">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical">

                                        <include layout="@layout/item_simmer_visitors_layout" />

                                        <include layout="@layout/item_simmer_visitors_layout" />

                                        <include layout="@layout/item_simmer_visitors_layout" />

                                        <include layout="@layout/item_simmer_visitors_layout" />

                                        <include layout="@layout/item_simmer_visitors_layout" />

                                        <include layout="@layout/item_simmer_visitors_layout" />

                                        <include layout="@layout/item_simmer_visitors_layout" />

                                        <include layout="@layout/item_simmer_visitors_layout" />

                                </LinearLayout>

                        </com.facebook.shimmer.ShimmerFrameLayout>
                        </RelativeLayout>
                </RelativeLayout>
        </androidx.core.widget.NestedScrollView>
        </androidx.swiperefreshlayout.widget.SwipeRefreshLayout>
        <ImageView
            android:id="@+id/pdfImage"
            android:layout_width="@dimen/_100sdp"
            android:layout_height="@dimen/_100sdp"></ImageView>
        <RelativeLayout
            android:visibility="gone"
            android:id="@+id/contactTxtLayout"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_35sdp"
            android:layout_alignParentBottom="true"
            android:background="#79000000">
                <ImageView
                    android:layout_marginStart="@dimen/_10sdp"
                    android:layout_marginEnd="@dimen/_10sdp"
                    android:layout_centerVertical="true"
                    android:layout_gravity="center_vertical"
                    android:id="@+id/whatsappImage"
                    android:layout_width="@dimen/_15sdp"
                    android:layout_height="@dimen/_15sdp"
                    android:src="@drawable/ic_whatsapp"
                    android:visibility="visible" />
                <TextView
                    android:layout_toEndOf="@+id/whatsappImage"
                    android:layout_centerVertical="true"
                    android:id="@+id/alertText"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:ellipsize="marquee"
                    android:focusable="true"
                    android:visibility="gone"
                    android:focusableInTouchMode="true"
                    android:fontFamily="@font/inter_medium"
                    android:gravity="center"
                    android:marqueeRepeatLimit="marquee_forever"
                    android:scrollHorizontally="true"
                    android:singleLine="true"
                    android:text="You don't have frame please contact to admin for your brand frame"
                    android:textColor="@color/white"
                    android:textSize="@dimen/_11sdp" />
        </RelativeLayout>

</RelativeLayout>
</layout>