<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/tools"
    xmlns:app1="http://schemas.android.com/apk/res-auto"
    app:ignore="NamespaceTypo">

    <RelativeLayout
        android:id="@+id/itemLayout"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/_40sdp"
        android:layout_marginLeft="@dimen/_3sdp"
        android:layout_marginRight="@dimen/_3sdp">

        <com.google.android.material.chip.Chip
            android:id="@+id/filterChip"
            app1:chipStartPadding="@dimen/_8sdp"
            app1:chipEndPadding="@dimen/_8sdp"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:elevation="@dimen/_2sdp"
            android:text="FilterData"
            android:textAppearance="@style/chipAppearance"
            android:textStyle="bold"
            app1:chipStrokeColor="@color/colorthird"
            app1:chipStrokeWidth="1dp" />

    </RelativeLayout>
</layout>