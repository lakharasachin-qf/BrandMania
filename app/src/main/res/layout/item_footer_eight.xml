<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <androidx.cardview.widget.CardView
            android:id="@+id/footerLayout"
            android:layout_width="@dimen/_128sdp"
            android:layout_height="@dimen/_128sdp"
            android:layout_marginStart="@dimen/_5sdp"
            android:layout_marginTop="@dimen/_5sdp"
            android:layout_marginEnd="@dimen/_5sdp"
            android:layout_marginBottom="@dimen/_5sdp"
            android:elevation="@dimen/_3sdp"
            app:cardCornerRadius="@dimen/_7sdp"
            app:cardElevation="@dimen/_4sdp"
            app:cardPreventCornerOverlap="false"
            app:cardUseCompatPadding="true"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:layout_marginTop="@dimen/_10sdp"

                >

                <RelativeLayout
                    android:id="@+id/BrandNameLayout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_5sdp">

                    <View

                        android:id="@+id/viewone"
                        android:layout_width="match_parent"
                        android:layout_height="0.5dp"
                        android:layout_centerVertical="true"
                        android:layout_toStartOf="@+id/brandNameText"
                        android:background="@color/colorPrimary"
                        android:backgroundTint="@color/colorPrimary" />

                    <TextView
                        android:id="@+id/brandNameText"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:layout_marginStart="@dimen/_10sdp"
                        android:layout_marginEnd="@dimen/_10sdp"
                        android:singleLine="true"
                        android:text="Brand Mghfghfania"
                        android:textColor="@color/colorPrimary"
                        android:textSize="@dimen/_5sdp" />

                    <View
                        android:id="@+id/topView2"
                        android:layout_width="match_parent"
                        android:layout_height="0.5dp"
                        android:layout_centerVertical="true"
                        android:layout_toEndOf="@+id/brandNameText"
                        android:background="@color/colorPrimary"
                        android:backgroundTint="@color/colorPrimary" />
                </RelativeLayout>

                <RelativeLayout

                    android:id="@+id/element1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/BrandNameLayout"
                    android:layout_centerHorizontal="true"
                    android:gravity="center_horizontal"
                    android:paddingStart="@dimen/_7sdp"
                    android:paddingEnd="@dimen/_7sdp">

                    <RelativeLayout
                        android:id="@+id/gmailLayout"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_toEndOf="@+id/element"
                        android:visibility="visible">

                        <ImageView

                            android:id="@+id/gmailImage"
                            android:layout_width="@dimen/_6sdp"
                            android:layout_height="@dimen/_6sdp"
                            android:layout_centerVertical="true"
                            android:layout_marginTop="@dimen/_1sdp"
                            android:src="@drawable/ic_outline_mail_24"
                            app:tint="@color/colorPrimary" />

                        <TextView
                            android:id="@+id/gmailText"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="@dimen/_3sdp"
                            android:layout_toEndOf="@+id/gmailImage"
                            android:includeFontPadding="false"
                            android:padding="0dp"
                            android:singleLine="true"
                            android:text="qfsolfddgdfgfdgdfgdgdfution@gmail.com"
                            android:textColor="@color/colorPrimary"
                            android:textSize="@dimen/_4sdp" />

                    </RelativeLayout>

                    <View
                        android:id="@+id/element"
                        android:layout_width="@dimen/_1sdp"
                        android:layout_height="@dimen/_5sdp"
                        android:layout_centerInParent="true"
                        android:layout_marginStart="@dimen/_7sdp"
                        android:layout_marginEnd="@dimen/_7sdp"
                        android:background="@color/colorPrimary"
                        android:visibility="visible" />

                    <RelativeLayout
                        android:id="@+id/contactLayout"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_toStartOf="@+id/element"
                        android:visibility="visible">

                        <ImageView
                            android:id="@+id/contactImage"
                            android:layout_width="@dimen/_6sdp"
                            android:layout_height="@dimen/_6sdp"
                            android:layout_centerVertical="true"
                            android:layout_marginTop="@dimen/_1sdp"
                            android:src="@drawable/ic_outline_call_24"
                            app:tint="@color/colorPrimary" />

                        <TextView
                            android:id="@+id/contactText"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_marginStart="@dimen/_3sdp"
                            android:layout_toEndOf="@+id/contactImage"
                            android:includeFontPadding="false"
                            android:padding="0dp"
                            android:singleLine="true"

                            android:text="+91 8849896537"
                            android:textColor="@color/colorPrimary"
                            android:textSize="@dimen/_4sdp" />

                    </RelativeLayout>
                </RelativeLayout>


                <RelativeLayout
                    android:id="@+id/addressLayoutElement"

                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/element1"
                    android:layout_centerHorizontal="true"
                    android:layout_marginBottom="@dimen/_10sdp"

                    >

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_centerVertical="true"
                        android:layout_marginTop="@dimen/_5sdp"
                        android:gravity="center_horizontal">

                        <ImageView

                            android:id="@+id/loacationImage"
                            android:layout_width="@dimen/_6sdp"
                            android:layout_height="@dimen/_6sdp"
                            android:layout_alignParentTop="true"
                            android:layout_centerVertical="true"
                            android:layout_marginStart="@dimen/_10sdp"
                            android:layout_marginTop="@dimen/_1sdp"
                            android:src="@drawable/ic_outline_location_on_24"
                            app:tint="@color/colorPrimary" />

                        <TextView
                            android:id="@+id/locationText"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_marginStart="@dimen/_5sdp"
                            android:layout_marginEnd="@dimen/_10sdp"
                            android:layout_toEndOf="@+id/loacationImage"
                            android:includeFontPadding="false"
                            android:text="326,Rudra Complex Vastral"
                            android:textColor="@color/colorPrimary"
                            android:textSize="@dimen/_4sdp" />
                    </RelativeLayout>
                </RelativeLayout>

            </RelativeLayout>

        </androidx.cardview.widget.CardView>



        <androidx.cardview.widget.CardView
            android:id="@+id/elementSelected"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_5sdp"
            android:layout_marginEnd="@dimen/_10sdp"
            android:visibility="gone"
            app:cardBackgroundColor="@color/colorthird"
            app:cardCornerRadius="@dimen/_3sdp"
            app:cardElevation="@dimen/_4sdp"
            app:layout_constraintEnd_toEndOf="@id/footerLayout"
            app:layout_constraintTop_toTopOf="parent">

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingStart="@dimen/_5sdp"
                android:paddingTop="@dimen/_2sdp"
                android:paddingEnd="@dimen/_5sdp"
                android:paddingBottom="@dimen/_2sdp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/inter_regular"
                    android:text="Selected"
                    android:textColor="@color/black"
                    android:textSize="@dimen/_6sdp"
                    android:textStyle="bold" />

                <ImageView
                    android:layout_width="@dimen/_15sdp"
                    android:layout_height="@dimen/_15sdp"
                    android:background="@color/colorPrimary"
                    android:elevation="@dimen/_20sdp"
                    android:src="@drawable/cpv_preset_checked"
                    android:visibility="gone" />
            </RelativeLayout>
        </androidx.cardview.widget.CardView>

        <RelativeLayout
            android:id="@+id/elementPremium"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_15sdp"
            android:layout_marginEnd="@dimen/_10sdp"
            android:elevation="@dimen/_10sdp"
            android:paddingBottom="@dimen/_5sdp"
            android:visibility="gone"
            app:layout_constraintEnd_toEndOf="@id/footerLayout"
            app:layout_constraintTop_toTopOf="parent">

            <ImageView
                android:layout_width="@dimen/_15sdp"
                android:layout_height="@dimen/_15sdp"
                android:elevation="@dimen/_20sdp"
                android:src="@drawable/ic_crown" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/freePremium"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_18sdp"
            android:layout_marginEnd="@dimen/_10sdp"
            android:elevation="@dimen/_10sdp"
            android:visibility="gone"

            app:layout_constraintEnd_toEndOf="@id/footerLayout"
            app:layout_constraintTop_toTopOf="parent">

            <ImageView
                android:layout_width="@dimen/_15sdp"
                android:layout_height="@dimen/_15sdp"
                android:elevation="@dimen/_20sdp"
                android:src="@drawable/ic_freee" />
        </RelativeLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>