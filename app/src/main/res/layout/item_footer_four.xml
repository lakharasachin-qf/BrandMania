<?xml version="1.0" encoding="utf-8"?>
<layout>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <view
            android:layout_below="@+id/topView"
            android:id="@+id/topView2"
            android:backgroundTint="@color/colorPrimary"
            android:layout_marginRight="@dimen/_20sdp"
            android:layout_width="@dimen/_5sdp"
            android:layout_height="@dimen/_80sdp">
        </view>
        <RelativeLayout
            android:layout_marginLeft="@dimen/_10sdp"
            android:layout_alignParentStart="true"
            android:id="@+id/gmailLayout"
            android:layout_toEndOf="@+id/topView2"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/_20sdp">
            <ImageView
                android:layout_marginTop="@dimen/_1sdp"

                android:id="@+id/gmailImage"
                android:src="@drawable/ic_outline_mail_24"
                android:layout_width="@dimen/_15sdp"
                android:layout_height="@dimen/_15sdp">
            </ImageView>
            <TextView

                android:layout_toEndOf="@+id/gmailImage"
                android:textSize="@dimen/_10sdp"
                android:layout_marginLeft="@dimen/_5sdp"
                android:id="@+id/gmailText"
                android:textColor="@color/colorPrimary"
                android:text="qfsolution@gmail.com"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
            </TextView>

        </RelativeLayout>
        <RelativeLayout
            android:layout_below="@+id/gmailLayout"
            android:layout_marginLeft="@dimen/_10sdp"
            android:layout_alignParentStart="true"
            android:id="@+id/websiteLayout"
            android:layout_toEndOf="@+id/topView2"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/_20sdp">
            <ImageView
                android:layout_marginTop="@dimen/_1sdp"
                android:backgroundTint="@color/colorPrimary"
                android:id="@+id/websiteImage"
                android:background="@drawable/ic_outline_language_24"
                android:layout_width="@dimen/_15sdp"
                android:layout_height="@dimen/_15sdp">
            </ImageView>
            <TextView

                android:layout_toEndOf="@+id/websiteImage"
                android:textSize="@dimen/_10sdp"
                android:layout_marginLeft="@dimen/_5sdp"
                android:id="@+id/websiteText"
                android:textColor="@color/colorPrimary"
                android:text="qfsolution.com"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
            </TextView>

        </RelativeLayout>
        <RelativeLayout
            android:layout_below="@+id/websiteLayout"
            android:layout_marginLeft="@dimen/_10sdp"
            android:layout_alignParentStart="true"
            android:layout_toEndOf="@+id/topView2"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/_20sdp"
            android:id="@+id/contactLayout"
           >
            <ImageView
                android:layout_marginTop="@dimen/_1sdp"

                android:id="@+id/contactImage"
                android:src="@drawable/ic_outline_call_24"
                android:layout_width="@dimen/_15sdp"
                android:layout_height="@dimen/_15sdp">
            </ImageView>
            <TextView
                android:layout_toEndOf="@+id/contactImage"
                android:textSize="@dimen/_10sdp"
                android:layout_marginLeft="@dimen/_5sdp"
                android:id="@+id/contactText"
                android:textColor="@color/colorPrimary"
                android:text="8849896537"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
            </TextView>

        </RelativeLayout>
        <RelativeLayout
            android:layout_below="@+id/contactLayout"
            android:layout_marginLeft="@dimen/_10sdp"
            android:layout_alignParentStart="true"
            android:layout_toEndOf="@+id/topView2"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/_20sdp"
            android:id="@+id/locationLayout">
            <ImageView
                android:layout_centerVertical="true"
                android:id="@+id/loacationImage"
                android:backgroundTint="@color/colorPrimary"
                android:background="@drawable/ic_outline_location_on_24"
                android:layout_width="@dimen/_15sdp"
                android:layout_height="@dimen/_15sdp">
            </ImageView>
            <TextView
                android:layout_centerVertical="true"
                android:layout_toEndOf="@+id/loacationImage"
                android:textSize="@dimen/_10sdp"
                android:layout_marginLeft="@dimen/_5sdp"
                android:id="@+id/locationText"
                android:textColor="@color/colorPrimary"
                android:text="Vastral"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
            </TextView>
        </RelativeLayout>
    </RelativeLayout>
</RelativeLayout>
</layout>