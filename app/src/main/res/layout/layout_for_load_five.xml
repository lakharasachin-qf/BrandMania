<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <RelativeLayout
        android:paddingBottom="@dimen/_10sdp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <RelativeLayout
                android:orientation="vertical"
                android:layout_centerVertical="true"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingTop="@dimen/_10sdp"
                android:paddingBottom="@dimen/_10sdp">



            <RelativeLayout
                android:id="@+id/main"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_20sdp">

            <RelativeLayout
                android:layout_marginLeft="@dimen/_120sdp"
                android:id="@+id/element0"
                android:layout_centerVertical="true"
                android:layout_width="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_20sdp"
                android:orientation="horizontal">

                <ImageView
                    android:layout_centerVertical="true"
                    android:id="@+id/websiteImage"
                    android:layout_gravity="center_vertical"
                    android:layout_width="@dimen/_12sdp"
                    android:layout_height="@dimen/_12sdp"
                    android:src="@drawable/ic_outline_language_24"
                    app:tint="@color/colorPrimary" />

                <TextView
                    android:layout_toEndOf="@id/websiteImage"
                    android:id="@+id/websiteText"
                    android:layout_centerVertical="true"
                    android:layout_gravity="center_vertical"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_5sdp"
                    android:text="Website"
                    android:textSize="@dimen/_8sdp" />
            </RelativeLayout>
            </RelativeLayout>
            <View

                android:id="@+id/viewElement2"
                android:layout_width="match_parent"
                android:layout_height="1dp"
               android:layout_below="@+id/main"
                android:layout_marginTop="@dimen/_4sdp"
                android:layout_marginBottom="@dimen/_4sdp"
                android:background="@color/colorPrimary" />

            <RelativeLayout
                android:layout_centerVertical="true"
                android:layout_below="@+id/viewElement2"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_20sdp">

                <ImageView
                    android:id="@+id/element1"
                    android:layout_width="@dimen/_30sdp"
                    android:layout_height="@dimen/_20sdp"
                    android:layout_alignParentStart="true"
                    android:src="@drawable/shape_frame" />

                <RelativeLayout
                    android:layout_toRightOf="@+id/element3"
                    android:id="@+id/element2"
                    android:layout_centerVertical="true"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/_20sdp"
                    android:layout_toEndOf="@+id/element1"
                    android:layout_marginStart="@dimen/_5sdp"
                    android:layout_marginEnd="@dimen/_5sdp"
                    android:gravity="center_vertical"
                   >

                    <RelativeLayout
                        android:layout_centerVertical="true"
                        android:id="@+id/elementMobile"
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/_20sdp"
                        >

                        <ImageView
                            android:layout_centerVertical="true"
                            android:id="@+id/contactImage"
                            android:layout_width="@dimen/_12sdp"
                            android:layout_height="@dimen/_12sdp"
                            android:layout_gravity="center_vertical"
                            android:src="@drawable/ic_phone"
                            app:tint="@color/colorPrimary" />

                        <TextView
                            android:layout_toEndOf="@+id/contactImage"
                            android:id="@+id/phoneTxt"
                            android:layout_gravity="center_vertical"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:singleLine="true"
                            android:layout_centerVertical="true"
                            android:maxWidth="@dimen/_100sdp"
                            android:minWidth="@dimen/_70sdp"
                            android:text="+91 8200 0026 04"
                            android:layout_marginStart="@dimen/_3sdp"
                            android:textSize="@dimen/_8sdp" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/_120sdp"
                        android:id="@+id/elementEmail"
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/_20sdp"

                       >

                        <ImageView
                            android:layout_centerVertical="true"
                            android:id="@+id/gmailImage"
                            android:layout_gravity="center_vertical"
                            android:layout_width="@dimen/_12sdp"
                            android:layout_height="@dimen/_12sdp"
                            android:src="@drawable/ic_outline_mail_24"
                            app:tint="@color/colorPrimary" />

                        <TextView
                            android:layout_centerVertical="true"
                            android:layout_toEndOf="@+id/gmailImage"
                            android:id="@+id/gmailText"
                            android:layout_gravity="center_vertical"
                            android:maxWidth="@dimen/_120sdp"
                            android:singleLine="true"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_5sdp"
                            android:text="lakharasachin.qf@gmail.com"
                            android:textSize="@dimen/_8sdp" />
                    </RelativeLayout>
                </RelativeLayout>

                <ImageView
                    android:id="@+id/element3"
                    android:layout_width="@dimen/_30sdp"
                    android:layout_height="@dimen/_20sdp"
                    android:layout_alignParentEnd="true"
                    android:src="@drawable/shape_frame" />

            </RelativeLayout>

        </RelativeLayout>

    </RelativeLayout>
</layout>