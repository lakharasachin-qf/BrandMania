<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:shimmer="http://schemas.android.com/apk/res-auto">
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Activity.ViewOnlyCustomeFrame">




    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <androidx.appcompat.widget.Toolbar
            android:id="@+id/my_toolbar"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_40sdp"
            android:background="@color/colorPrimary"
            android:elevation="4dp"
            android:theme="@style/ThemeOverlay.AppCompat.ActionBar"
            android:visibility="visible"
            app:popupTheme="@style/ThemeOverlay.AppCompat.Light">

            <RelativeLayout
                android:id="@+id/main_linearlayout_title"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_70sdp"
                android:layout_marginTop="@dimen/_10sdp"
                android:layout_marginRight="@dimen/_7sdp"
                android:elevation="0dp">

                <RelativeLayout

                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_70sdp"
                    android:layout_marginRight="@dimen/_7sdp"
                    android:elevation="0dp">

                    <ImageView
                        android:id="@+id/backIcon"
                        android:layout_width="23dp"
                        android:layout_height="23dp"
                        android:layout_alignParentLeft="true"
                        android:layout_marginLeft="6dp"
                        android:layout_marginRight="10dp"
                        android:background="@drawable/ic_left_arrow" />

                    <TextView
                        android:id="@+id/titleName"
                        android:layout_width="@dimen/_120sdp"
                        android:layout_height="wrap_content"
                        android:layout_toEndOf="@+id/backIcon"
                        android:ellipsize="marquee"
                        android:focusable="true"
                        android:focusableInTouchMode="true"
                        android:fontFamily="@font/montserrat_semibold"
                        android:marqueeRepeatLimit="marquee_forever"
                        android:scrollHorizontally="true"
                        android:singleLine="true"
                        android:text="My Busineshfghfghfghfgfhghfhfs"
                        android:textColor="@color/colorNavText"
                        android:textSize="@dimen/_13sdp"
                        android:textStyle="bold" />

                </RelativeLayout>

                <ImageView
                    android:visibility="gone"
                    android:id="@+id/videoTutorial"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/_10sdp"
                    android:layout_toStartOf="@+id/favSection"
                    android:fontFamily="@font/inter_semibold"
                    android:src="@drawable/ic_play_button"
                    android:textColor="@color/colorNavText"
                    android:textSize="@dimen/_14sdp"
                    android:textStyle="bold"
                    app:tint="@color/white" />

                <ImageView
                    android:id="@+id/downloadIcon"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_marginLeft="@dimen/_10sdp"
                    android:fontFamily="@font/inter_semibold"
                    android:src="@drawable/ic_baseline_cloud_download_24"
                    android:textColor="@color/colorNavText"
                    android:textSize="@dimen/_14sdp"
                    android:textStyle="bold" />

                <ImageView
                    android:id="@+id/shareIcon"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/_10sdp"
                    android:layout_toLeftOf="@+id/downloadIcon"
                    android:fontFamily="@font/inter_semibold"
                    android:src="@drawable/ic_baseline_share_24"
                    android:textColor="@color/colorNavText"
                    android:textSize="@dimen/_14sdp"
                    android:textStyle="bold" />
                <RelativeLayout
                    android:id="@+id/favSection"
                    android:layout_marginLeft="@dimen/_10sdp"
                    android:layout_toLeftOf="@+id/shareIcon"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                    <ImageView
                        android:id="@+id/fabroutIcon"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/inter_semibold"
                        android:src="@drawable/ic_baseline_favorite_border_24"
                        android:textColor="@color/colorNavText"
                        android:textSize="@dimen/_14sdp"
                        android:textStyle="bold" />
                    <ImageView
                        android:id="@+id/addfabroutIcon"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"

                        android:fontFamily="@font/inter_semibold"
                        android:src="@drawable/ic_baseline_favorite_24"
                        android:textColor="@color/colorNavText"
                        android:textSize="@dimen/_14sdp"
                        android:textStyle="bold"
                        android:visibility="gone" />
                </RelativeLayout>




            </RelativeLayout>
        </androidx.appcompat.widget.Toolbar>

    <androidx.cardview.widget.CardView
        android:layout_below="@+id/my_toolbar"
        android:id="@+id/cardView"
        android:layout_marginLeft="@dimen/_25sdp"
        android:layout_marginRight="@dimen/_25sdp"
        android:layout_marginTop="@dimen/_25sdp"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_280sdp">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">
        <RelativeLayout
            android:id="@+id/frameShowRelative"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_250sdp">
            <ImageView
                android:id="@+id/frameShow"
                android:background="@color/colorPrimary"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_250sdp">
            </ImageView>
            <TextView
                android:id="@+id/brandName"
                android:layout_alignParentBottom="true"
                android:layout_marginBottom="@dimen/_10sdp"
                android:text="Brane Name"
                android:textColor="@color/black"
                android:textSize="@dimen/_13sdp"
                android:layout_centerHorizontal="true"
                android:fontFamily="@font/inter_semibold"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
            </TextView>

        </RelativeLayout>
        <TextView
            android:layout_below="@+id/frameShowRelative"
            android:text="EDIT"
            android:textStyle="bold"
            android:fontFamily="@font/inter_semibold"
            android:layout_centerHorizontal="true"
            android:gravity="center"
            android:layout_marginTop="@dimen/_15sdp"
            android:textColor="@color/colorPrimary"
            android:textSize="@dimen/_12sdp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

        </TextView>
        </RelativeLayout>
    </androidx.cardview.widget.CardView>

     <androidx.recyclerview.widget.RecyclerView
         android:id="@+id/customeFrameRelative"
         android:layout_marginLeft="@dimen/_10sdp"
         android:layout_marginRight="@dimen/_10sdp"
         android:layout_below="@+id/cardView"
         android:layout_marginTop="@dimen/_10sdp"
         android:layout_width="match_parent"
         android:layout_height="match_parent">

     </androidx.recyclerview.widget.RecyclerView>
        <com.facebook.shimmer.ShimmerFrameLayout

            android:id="@+id/shimmerForPagination"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:orientation="vertical"
            android:layout_below="@+id/cardView"
            shimmer:shimmer_duration="800">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <include layout="@layout/item_simmer_visitors_layout" />
                <include layout="@layout/item_simmer_visitors_layout" />
            </LinearLayout>

        </com.facebook.shimmer.ShimmerFrameLayout>




    </RelativeLayout>
</RelativeLayout>
</layout>