<?xml version="1.0" encoding="utf-8"?>
<layout>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorNavText"
    xmlns:app="http://schemas.android.com/apk/res-auto">
        <androidx.appcompat.widget.Toolbar
            android:visibility="visible"
            android:id="@+id/my_toolbar"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_70sdp"
            android:background="@drawable/toolbar_background_second"
            android:elevation="4dp"
            android:theme="@style/ThemeOverlay.AppCompat.ActionBar"
            app:popupTheme="@style/ThemeOverlay.AppCompat.Light">

                <RelativeLayout
                    android:layout_marginTop="@dimen/_10sdp"
                    android:layout_marginRight="@dimen/_7sdp"
                    android:id="@+id/main_linearlayout_title"
                    android:elevation="0dp"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_70sdp">
                          <TextView
                            android:id="@+id/firsttitle"
                            android:text="@string/yourbusiness_text"
                            android:textColor="@color/colorNavText"
                            android:textSize="@dimen/_6sdp"
                            android:fontFamily="@font/montserratregular"
                            android:layout_width="wrap_content"
                            android:textStyle="bold"
                            android:layout_height="wrap_content">

                        </TextView>

                        <TextView
                            android:layout_below="@+id/firsttitle"
                            android:id="@+id/businessName"
                            android:text=""
                            android:textColor="@color/colorNavText"
                            android:textSize="@dimen/_11sdp"

                            android:fontFamily="@font/montserratsemibold"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">
                        </TextView>
                     <!--   <TextView
                            android:layout_toEndOf="@+id/backIcon"
                            android:layout_marginTop="@dimen/_15sdp"
                            android:id="@+id/firsttitle"
                            android:text="@string/yourbusiness_text"
                            android:textColor="@color/colorNavText"
                            android:textSize="@dimen/_8sdp"
                            android:fontFamily="@font/inter_regular"
                            android:layout_width="wrap_content"
                            android:textStyle="bold"
                            android:layout_marginLeft="@dimen/_12sdp"
                            android:layout_height="wrap_content">

                        </TextView>
                        <TextView
                            android:layout_marginLeft="@dimen/_10sdp"
                            android:layout_toEndOf="@+id/backIcon"
                            android:id="@+id/businessName"
                            android:text="QueryFinderSolution"
                            android:textColor="@color/colorNavText"
                            android:textSize="@dimen/_13sdp"

                            android:fontFamily="@font/inter_extrabold"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">
                        </TextView>-->
                        <ImageView
                            android:visibility="gone"
                            android:id="@+id/backIcon"
                            android:layout_width="@dimen/_20sdp"
                            android:layout_alignParentStart="true"
                            android:src="@drawable/ic_left_arrow"
                            android:layout_height="@dimen/_20sdp">

                        </ImageView>

                </RelativeLayout>
        </androidx.appcompat.widget.Toolbar>

        <ImageView
            android:layout_marginStart="-100dp"
            android:layout_alignParentBottom="true"

            android:src="@drawable/bottom"
            android:layout_width="@dimen/_300sdp"
            android:layout_height="@dimen/_300sdp">

        </ImageView>

        <ScrollView
            android:layout_below="@+id/my_toolbar"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:scrollbars="none">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_below="@+id/my_toolbar"
                            android:orientation="vertical">

                                <RelativeLayout
                                    android:id="@+id/mybusinessRelative"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="@dimen/_15sdp"
                                    android:layout_marginTop="@dimen/_20sdp"
                                    android:layout_marginEnd="@dimen/_15sdp"
                                    android:paddingTop="@dimen/_7sdp"
                                    android:paddingBottom="@dimen/_7sdp">

                                        <ImageView
                                            android:id="@+id/myBusinessImage"
                                            android:layout_width="@dimen/_20sdp"
                                            android:layout_height="@dimen/_20sdp"
                                            android:src="@drawable/ic_portfolio_outline"></ImageView>

                                        <ImageView
                                            android:layout_width="@dimen/_15sdp"
                                            android:layout_height="@dimen/_15sdp"
                                            android:layout_alignParentEnd="true"
                                            android:padding="@dimen/_2sdp"
                                            android:src="@drawable/ic_next"></ImageView>

                                        <TextView
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_centerVertical="true"
                                            android:layout_marginStart="@dimen/_10sdp"
                                            android:layout_toEndOf="@+id/myBusinessImage"
                                            android:fontFamily="@font/montserratmedium"
                                            android:text="@string/mybusinesse_text"
                                            android:textColor="@android:color/black"
                                            android:textSize="@dimen/_12sdp" />

                                </RelativeLayout>

                                <View
                                    android:layout_width="match_parent"
                                    android:layout_height="1dp"
                                    android:background="@color/Graycolor" />

                                <RelativeLayout
                                    android:id="@+id/myFaqRelative"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="@dimen/_15sdp"
                                    android:layout_marginTop="@dimen/_4sdp"
                                    android:layout_marginEnd="@dimen/_15sdp"
                                    android:paddingTop="@dimen/_7sdp"
                                    android:paddingBottom="@dimen/_7sdp">

                                        <ImageView
                                            android:id="@+id/myFaqImage"
                                            android:layout_width="@dimen/_20sdp"
                                            android:layout_height="@dimen/_20sdp"
                                            android:src="@drawable/ic_about"></ImageView>

                                        <ImageView
                                            android:layout_width="@dimen/_15sdp"
                                            android:layout_height="@dimen/_15sdp"
                                            android:layout_alignParentEnd="true"

                                            android:padding="@dimen/_2sdp"
                                            android:src="@drawable/ic_next"></ImageView>

                                        <TextView
                                            android:id="@+id/faqactivity"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_centerVertical="true"
                                            android:layout_marginLeft="@dimen/_10sdp"
                                            android:layout_toEndOf="@+id/myFaqImage"
                                            android:fontFamily="@font/montserratmedium"
                                            android:text="@string/faq_text"
                                            android:textColor="@android:color/black"
                                            android:textSize="@dimen/_12sdp" />

                                </RelativeLayout>

                                <View
                                    android:layout_width="match_parent"
                                    android:layout_height="1dp"
                                    android:background="@color/Graycolor" />

                                <RelativeLayout
                                    android:id="@+id/partnerProgRelative"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/_4sdp"
                                    android:paddingTop="@dimen/_7sdp"
                                    android:paddingBottom="@dimen/_7sdp"
                                    android:visibility="gone">

                                        <ImageView
                                            android:layout_width="@dimen/_15sdp"
                                            android:layout_height="@dimen/_15sdp"
                                            android:layout_alignParentEnd="true"
                                            android:layout_marginLeft="@dimen/_10sdp"
                                            android:layout_marginRight="@dimen/_10sdp"
                                            android:src="@drawable/ic_next"></ImageView>

                                        <ImageView
                                            android:id="@+id/partnerProgImage"
                                            android:layout_width="@dimen/_20sdp"
                                            android:layout_height="@dimen/_20sdp"

                                            android:layout_marginStart="@dimen/_10sdp"
                                            android:padding="@dimen/_2sdp"
                                            android:src="@drawable/ic_hand_shake"></ImageView>

                                        <TextView
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_centerVertical="true"
                                            android:layout_marginLeft="@dimen/_10sdp"
                                            android:layout_toEndOf="@+id/partnerProgImage"
                                            android:fontFamily="@font/inter_regular"
                                            android:text="@string/partnerprogram_text"
                                            android:textColor="@android:color/black"
                                            android:textSize="@dimen/_12sdp"></TextView>

                                </RelativeLayout>

                                <View
                                    android:layout_width="match_parent"
                                    android:layout_height="1dp"
                                    android:background="@color/Graycolor"
                                    android:visibility="gone" />

                                <RelativeLayout
                                    android:id="@+id/helpandsupportLayout"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/_4sdp"
                                    android:paddingTop="@dimen/_7sdp"
                                    android:paddingBottom="@dimen/_7sdp"
                                    android:visibility="gone">

                                        <ImageView
                                            android:layout_width="@dimen/_15sdp"
                                            android:layout_height="@dimen/_15sdp"
                                            android:layout_alignParentEnd="true"
                                            android:layout_marginRight="@dimen/_10sdp"
                                            android:src="@drawable/ic_next"></ImageView>

                                        <ImageView
                                            android:id="@+id/helpandsupportImage"
                                            android:layout_width="@dimen/_20sdp"
                                            android:layout_height="@dimen/_20sdp"
                                            android:layout_marginLeft="@dimen/_10sdp"
                                            android:padding="@dimen/_2sdp"

                                            android:src="@drawable/ic_supporttt"></ImageView>

                                        <TextView
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_centerVertical="true"
                                            android:layout_marginLeft="@dimen/_10sdp"
                                            android:layout_toEndOf="@+id/helpandsupportImage"
                                            android:fontFamily="@font/inter_regular"
                                            android:text="@string/helpandsupport_text"
                                            android:textColor="@android:color/black"
                                            android:textSize="@dimen/_12sdp" />

                                </RelativeLayout>

                                <View
                                    android:layout_width="match_parent"
                                    android:layout_height="1dp"
                                    android:background="@color/Graycolor"
                                    android:visibility="gone" />

                                <RelativeLayout
                                    android:id="@+id/shareText"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="@dimen/_15sdp"
                                    android:layout_marginTop="@dimen/_4sdp"
                                    android:layout_marginEnd="@dimen/_15sdp"
                                    android:paddingTop="@dimen/_7sdp"
                                    android:paddingBottom="@dimen/_7sdp">

                                        <ImageView
                                            android:layout_width="@dimen/_15sdp"
                                            android:layout_height="@dimen/_15sdp"
                                            android:layout_alignParentEnd="true"
                                            android:layout_marginLeft="@dimen/_10sdp"

                                            android:padding="@dimen/_2sdp"
                                            android:src="@drawable/ic_next"></ImageView>

                                        <ImageView
                                            android:id="@+id/shareImage"
                                            android:layout_width="@dimen/_20sdp"
                                            android:layout_height="@dimen/_20sdp"
                                            android:src="@drawable/ic_share"></ImageView>

                                        <TextView
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_centerVertical="true"
                                            android:layout_marginLeft="@dimen/_10sdp"
                                            android:layout_toEndOf="@+id/shareImage"
                                            android:fontFamily="@font/montserratmedium"
                                            android:text="@string/shareapp_text"
                                            android:textColor="@android:color/black"
                                            android:textSize="@dimen/_12sdp" />

                                </RelativeLayout>

                                <View
                                    android:layout_width="match_parent"
                                    android:layout_height="1dp"
                                    android:background="@color/Graycolor" />

                                <RelativeLayout
                                    android:id="@+id/rateUsLayout"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="@dimen/_15sdp"
                                    android:layout_marginTop="@dimen/_4sdp"
                                    android:layout_marginEnd="@dimen/_15sdp"
                                    android:paddingTop="@dimen/_7sdp"
                                    android:paddingBottom="@dimen/_7sdp"
                                    android:visibility="visible">

                                        <ImageView
                                            android:layout_width="@dimen/_15sdp"
                                            android:layout_height="@dimen/_15sdp"
                                            android:layout_alignParentEnd="true"

                                            android:padding="@dimen/_2sdp"
                                            android:src="@drawable/ic_next"></ImageView>

                                        <ImageView
                                            android:id="@+id/rateusImage"
                                            android:layout_width="@dimen/_20sdp"
                                            android:layout_height="@dimen/_20sdp"
                                            android:src="@drawable/ic_report"></ImageView>

                                        <TextView
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_centerVertical="true"
                                            android:layout_marginLeft="@dimen/_10sdp"
                                            android:layout_toEndOf="@+id/rateusImage"
                                            android:fontFamily="@font/montserratmedium"
                                            android:text="Rate Us"
                                            android:textColor="@android:color/black"
                                            android:textSize="@dimen/_12sdp"></TextView>

                                </RelativeLayout>

                                <View
                                    android:layout_width="match_parent"
                                    android:layout_height="1dp"
                                    android:background="@color/Graycolor"
                                    android:visibility="visible" />

                                <RelativeLayout
                                    android:id="@+id/aboutUsRelative"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="@dimen/_15sdp"
                                    android:layout_marginTop="@dimen/_4sdp"
                                    android:layout_marginEnd="@dimen/_15sdp"
                                    android:paddingTop="@dimen/_7sdp"
                                    android:paddingBottom="@dimen/_7sdp">

                                        <ImageView
                                            android:layout_width="@dimen/_15sdp"
                                            android:layout_height="@dimen/_15sdp"

                                            android:layout_alignParentEnd="true"
                                            android:padding="@dimen/_2sdp"
                                            android:src="@drawable/ic_next"></ImageView>

                                        <ImageView
                                            android:id="@+id/aboutUsImage"
                                            android:layout_width="@dimen/_20sdp"
                                            android:layout_height="@dimen/_20sdp"
                                            android:src="@drawable/ic_hand_shake"></ImageView>

                                        <TextView
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_centerVertical="true"
                                            android:layout_marginLeft="@dimen/_10sdp"
                                            android:layout_toEndOf="@+id/aboutUsImage"
                                            android:fontFamily="@font/montserratmedium"
                                            android:text="About Us"
                                            android:textColor="@android:color/black"
                                            android:textSize="@dimen/_12sdp" />

                                </RelativeLayout>

                                <View
                                    android:layout_width="match_parent"
                                    android:layout_height="1dp"
                                    android:background="@color/Graycolor" />


                                <RelativeLayout
                                    android:id="@+id/reportbugsLayout"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="@dimen/_15sdp"
                                    android:layout_marginTop="@dimen/_4sdp"
                                    android:layout_marginEnd="@dimen/_15sdp"
                                    android:paddingTop="@dimen/_7sdp"
                                    android:paddingBottom="@dimen/_7sdp">

                                        <ImageView
                                            android:id="@+id/subscriptionImage"
                                            android:layout_width="@dimen/_20sdp"
                                            android:layout_height="@dimen/_20sdp"
                                            android:src="@drawable/ic_bug" />

                                        <ImageView
                                            android:layout_width="@dimen/_15sdp"
                                            android:layout_height="@dimen/_15sdp"
                                            android:layout_alignParentEnd="true"
                                            android:layout_marginLeft="@dimen/_10sdp"

                                            android:padding="@dimen/_2sdp"
                                            android:src="@drawable/ic_next"></ImageView>

                                        <TextView
                                            android:id="@+id/reportAndBug"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_centerVertical="true"
                                            android:layout_marginLeft="@dimen/_10sdp"
                                            android:layout_toEndOf="@+id/subscriptionImage"
                                            android:fontFamily="@font/montserratmedium"
                                            android:text="Report Bug"
                                            android:textColor="@android:color/black"
                                            android:textSize="@dimen/_12sdp" />

                                </RelativeLayout>

                                <View
                                    android:layout_width="match_parent"
                                    android:layout_height="1dp"
                                    android:background="@color/Graycolor" />

                                <RelativeLayout
                                    android:id="@+id/packageRelative"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="@dimen/_15sdp"
                                    android:layout_marginTop="@dimen/_4sdp"
                                    android:layout_marginEnd="@dimen/_15sdp"
                                    android:paddingTop="@dimen/_7sdp"
                                    android:paddingBottom="@dimen/_7sdp">

                                        <ImageView
                                            android:layout_width="@dimen/_15sdp"
                                            android:layout_height="@dimen/_15sdp"
                                            android:layout_alignParentEnd="true"

                                            android:padding="@dimen/_2sdp"
                                            android:src="@drawable/ic_next"></ImageView>

                                        <ImageView
                                            android:id="@+id/primeImage"
                                            android:layout_width="@dimen/_20sdp"
                                            android:layout_height="@dimen/_20sdp"
                                            android:src="@drawable/ic_premiumnew"></ImageView>

                                        <TextView
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_centerVertical="true"
                                            android:layout_marginLeft="@dimen/_10sdp"
                                            android:layout_toEndOf="@+id/primeImage"
                                            android:fontFamily="@font/montserratmedium"
                                            android:text="Package"
                                            android:textColor="@android:color/black"
                                            android:textSize="@dimen/_12sdp" />

                                </RelativeLayout>

                                <View
                                    android:layout_width="match_parent"
                                    android:layout_height="1dp"
                                    android:background="@color/Graycolor" />

                                <RelativeLayout
                                    android:id="@+id/logoutRelative"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="@dimen/_15sdp"
                                    android:layout_marginTop="@dimen/_4sdp"
                                    android:layout_marginEnd="@dimen/_15sdp"
                                    android:paddingTop="@dimen/_7sdp"
                                    android:paddingBottom="@dimen/_7sdp">

                                        <ImageView
                                            android:layout_width="@dimen/_15sdp"
                                            android:layout_height="@dimen/_15sdp"
                                            android:layout_alignParentEnd="true"

                                            android:padding="@dimen/_2sdp"
                                            android:src="@drawable/ic_next"></ImageView>

                                        <ImageView
                                            android:id="@+id/logoutImage"
                                            android:layout_width="@dimen/_20sdp"
                                            android:layout_height="@dimen/_20sdp"
                                            android:src="@drawable/ic_logout"></ImageView>

                                        <TextView
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_centerVertical="true"
                                            android:layout_marginStart="@dimen/_10sdp"
                                            android:layout_toEndOf="@+id/logoutImage"
                                            android:fontFamily="@font/montserratmedium"
                                            android:text="@string/logout"
                                            android:textColor="@android:color/black"
                                            android:textSize="@dimen/_12sdp" />

                                </RelativeLayout>

                                <View
                                    android:layout_width="match_parent"
                                    android:layout_height="1dp"
                                    android:background="@color/Graycolor" />

                        </LinearLayout>
                        <LinearLayout
                            android:layout_marginTop="@dimen/_80sdp"
                            android:id="@+id/contactTxtLayout"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_above="@+id/appVersionTxt"
                            android:gravity="center_horizontal"
                            android:orientation="vertical"
                            android:paddingTop="@dimen/_7sdp">

                                <ImageView

                                    android:id="@+id/whatsappImage"
                                    android:layout_width="@dimen/_25sdp"
                                    android:layout_height="@dimen/_25sdp"
                                    android:layout_gravity="center_horizontal"
                                    android:src="@drawable/ic_whatsapp"
                                    android:visibility="visible" />

                                <TextView
                                    android:id="@+id/reachUrTxt"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center_horizontal"
                                    android:layout_marginTop="@dimen/_3sdp"
                                    android:fontFamily="@font/montserratregular"
                                    android:text="Reach Us"

                                    android:textColor="@color/black"
                                    android:textSize="@dimen/_13sdp"
                                    android:textStyle="bold" />

                                <View
                                    android:layout_width="@dimen/_100sdp"
                                    android:layout_height="1dp"
                                    android:background="#B1B1B1"
                                    android:backgroundTint="@color/colorthird" />
                        </LinearLayout>
                        <TextView
                            android:id="@+id/appVersionTxt"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentBottom="true"
                            android:layout_centerHorizontal="true"
                            android:layout_gravity="center"
                            android:layout_marginTop="@dimen/_20sdp"
                            android:layout_marginBottom="@dimen/_60sdp"
                            android:fontFamily="@font/montserratregular"
                            android:text="App VErsion"
                            android:textSize="@dimen/_10sdp" />
                </LinearLayout>
        </ScrollView>

</RelativeLayout>
</layout>